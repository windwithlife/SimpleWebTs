{
    "name": "with-ant-design",
    "version": "1.0.0",
    "scripts": {
        "dev": "cross-env NODE_ENV=development nodemon",
        "clean": "rm -rf .next && rm -rf .server",
        "client:build": "next build",
        "server:build": "tsc --build tsconfig.server.json",
        "build": "npm run clean && npm run client:build && npm run server:build",
        "start": "sh ./scripts/startup.sh",
        "shutdown": "bash ./scripts/shutdown.sh",
        "mockpro": "bash ./scripts/mockpro.sh",
        "logstart": "bash ./scripts/logstart.sh"
    },
    "dependencies": {
