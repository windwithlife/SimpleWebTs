        "@popperjs/core": "2.9.1",
        "@typescript-eslint/parser": "^4.28.4",
        "@zeit/next-css": "^1.0.1",
        "@zeit/next-less": "^1.0.1",
        "antd": "^4.3.0",
        "axios": "^0.21.1",
        "body-parser": "^1.19.0",
        "chart.js": "2.9.4",
        "dayjs": "1.8.28",
        "echarts": "^5.1.2",
        "esm": "^3.2.25",
        "express": "^4.17.1",
        "fecha": "^4.2.1",
        "gulp": "4.0.2",
        "gulp-append-prepend": "1.0.8",
        "http-proxy": "^1.18.1",
        "less": "^4.1.1",
